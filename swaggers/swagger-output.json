{
   "openapi": "3.0.1",
   "info": {
      "version": "1.0.0",
      "title": "TADAK API",
      "description": "TADAK SERVER API"
   },
   "schemes": ["http"],
   "components": {
      "securitySchemes": {
         "accessTokenAuth": {
            "type": "apiKey",
            "in": "header",
            "name": "access-token",
            "description": "Enter your Access Token"
         }
      }
   },
   "paths": {
      "/user/login": {
         "post": {
            "description": "기기 로그인",
            "tags": ["User"],
            "summary": "로그인",
            "parameters": [
               {
                  "name": "device-id",
                  "in": "header",
                  "type": "string"
               }
            ],
            "responses": {
               "200": {
                  "description": "Login Success",
                  "content": {
                     "application/json": {
                        "schema": {
                           "type": "object",
                           "properties": {
                              "access_token": {
                                 "type": "string",
                                 "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6NCwiaWF0IjoxNzIyMjM1NzkxLCJleHAiOjE3MjIyMzU4NTF9.m7790_AD5P_oDTXCz4b9CwVK7pjn8b70UOZWojL4Kic"
                              },
                              "refresh_token": {
                                 "type": "string",
                                 "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6NCwiaWF0IjoxNzIyMjM1NzkxLCJleHAiOjE3MjIyMzYwOTF9.2WUst69Ta9qVPFDQ0HetHB1CsUt43Xg2sEkH_VGH8mM"
                              }
                           }
                        }
                     }
                  }
               }
            }
         }
      },
      "/user/refresh": {
         "post": {
            "description": "Refresh(Access Token 재발급)",
            "tags": ["User"],
            "summary": "Refresh",
            "parameters": [
               {
                  "name": "refresh-token",
                  "in": "header",
                  "type": "string",
                  "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6NCwiaWF0IjoxNzIyMjM1NzkxLCJleHAiOjE3MjIyMzU4NTF9.m7790_AD5P_oDTXCz4b9CwVK7pjn8b70UOZWojL4Kic"
               }
            ],
            "responses": {
               "200": {
                  "description": "Refresh Success(Access Token 재발급)",
                  "content": {
                     "application/json": {
                        "schema": {
                           "type": "object",
                           "properties": {
                              "access_token": {
                                 "type": "string",
                                 "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6NCwiaWF0IjoxNzIyMjM1NzkxLCJleHAiOjE3MjIyMzU4NTF9.m7790_AD5P_oDTXCz4b9CwVK7pjn8b70UOZWojL4Kic"
                              }
                           }
                        }
                     }
                  }
               },
               "401": {
                  "description": "Unauthorized",
                  "content": {
                     "application/json": {
                        "schema": {
                           "type": "object",
                           "properties": {
                              "message": {
                                 "type": "string",
                                 "example": "Refresh Token 이 만료되었습니다. or 유효하지 않습니다."
                              }
                           }
                        }
                     }
                  }
               },
               "500": {
                  "description": "Internal Server Error"
               }
            }
         }
      },
      "/user/nickname": {
         "put": {
            "description": "닉네임 수정",
            "tags": ["User"],
            "summary": "유저 닉네임 수정",
            "security": [{ "accessTokenAuth": [] }],
            "requestBody": {
               "content": {
                  "application/json": {
                     "schema": {
                        "type": "object",
                        "properties": {
                           "nickname": {
                              "type": "string",
                              "example": "변경할닉네임"
                           }
                        }
                     }
                  }
               }
            },
            "responses": {
               "200": {
                  "description": "닉네임 수정 완료",
                  "content": {
                     "application/json": {
                        "schema": {
                           "type": "object",
                           "properties": {
                              "message": {
                                 "type": "string",
                                 "example": "닉네임이 수정 완료 되었습니다."
                              }
                           }
                        }
                     }
                  }
               },
               "401": {
                  "description": "Unauthorized",
                  "content": {
                     "application/json": {
                        "schema": {
                           "type": "object",
                           "properties": {
                              "message": {
                                 "type": "string",
                                 "example": "message"
                              }
                           }
                        }
                     }
                  }
               },
               "500": {
                  "description": "Internal Server Error"
               }
            }
         }
      },
      "/account": {
         "post": {
            "description": "계좌 생성",
            "tags": ["Account"],
            "summary": "계좌 생성",
            "security": [{ "accessTokenAuth": [] }],
            "requestBody": {
               "content": {
                  "application/json": {
                     "schema": {
                        "type": "object",
                        "properties": {
                           "accountName": {
                              "type": "string",
                              "example": "삼성증권"
                           },
                           "securitiesCompanyId": {
                              "type": "number",
                              "example": 7
                           }
                        }
                     }
                  }
               }
            },
            "responses": {
               "200": {
                  "description": "계좌 생성 완료",
                  "content": {
                     "application/json": {
                        "schema": {
                           "type": "object",
                           "properties": {
                              "message": {
                                 "type": "string",
                                 "example": "새로운 계좌가 등록됐어요."
                              }
                           }
                        }
                     }
                  }
               },
               "400": {
                  "description": "Bad Request"
               },
               "401": {
                  "description": "Unauthorized"
               },
               "403": {
                  "description": "Forbidden"
               },
               "404": {
                  "description": "Not Found"
               },
               "500": {
                  "description": "Internal Server Error"
               }
            }
         },
         "put": {
            "description": "계좌 수정",
            "tags": ["Account"],
            "summary": "계좌 수정",
            "security": [{ "accessTokenAuth": [] }],
            "requestBody": {
               "content": {
                  "application/json": {
                     "schema": {
                        "type": "object",
                        "properties": {
                           "accountId": {
                              "type": "number",
                              "example": 17
                           },
                           "accountName": {
                              "type": "string",
                              "example": "삼성증권"
                           },
                           "securitiesCompanyId": {
                              "type": "number",
                              "example": 7
                           }
                        }
                     }
                  }
               }
            },
            "responses": {
               "200": {
                  "description": "계좌 수정 완료",
                  "content": {
                     "application/json": {
                        "schema": {
                           "type": "object",
                           "properties": {
                              "message": {
                                 "type": "string",
                                 "example": "계좌 내용이 수정됐어요."
                              }
                           }
                        }
                     }
                  }
               },
               "400": {
                  "description": "Bad Request"
               },
               "401": {
                  "description": "Unauthorized"
               },
               "403": {
                  "description": "Forbidden"
               },
               "404": {
                  "description": "Not Found"
               },
               "500": {
                  "description": "Internal Server Error"
               }
            }
         },
         "delete": {
            "description": "계좌 삭제",
            "tags": ["Account"],
            "summary": "계좌 삭제",
            "security": [{ "accessTokenAuth": [] }],
            "requestBody": {
               "content": {
                  "application/json": {
                     "schema": {
                        "type": "object",
                        "properties": {
                           "accountId": {
                              "type": "number",
                              "example": 17
                           }
                        }
                     }
                  }
               }
            },
            "responses": {
               "200": {
                  "description": "계좌 삭제 완료",
                  "content": {
                     "application/json": {
                        "schema": {
                           "type": "object",
                           "properties": {
                              "message": {
                                 "type": "string",
                                 "example": "변경된 내용이 저장됐어요."
                              }
                           }
                        }
                     }
                  }
               },
               "400": {
                  "description": "Bad Request"
               },
               "401": {
                  "description": "Unauthorized"
               },
               "403": {
                  "description": "Forbidden"
               },
               "404": {
                  "description": "Not Found"
               },
               "500": {
                  "description": "Internal Server Error"
               }
            }
         }
      }
   },
